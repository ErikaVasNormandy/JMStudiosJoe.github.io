&lt;script&gt;
function myFunction(imgs) {
  var expandImg = document.getElementById(&quot;expandedImg&quot;);
  var imgText = document.getElementById(&quot;imgtext&quot;);
  expandImg.src = imgs.src;
  imgText.innerHTML = imgs.alt;
  expandImg.parentElement.style.display = &quot;block&quot;;
}
&lt;/script&gt;

&lt;style&gt;
  small{
    font-size: 10px;
  }
  /* The expanding image container */
.container {
  display: none;
  z-index: 10;
  margin-left: auto;
  margin-right: auto;
  position: fixed;
  top: 10%;
  left: 10%;
  width: 80vw;
  overflow-y: scroll;
  overflow-x: scroll;
  bottom: 3%;
}
/* Expanding image text */
#imgtext {
  position: absolute;
  bottom: 15px;
  left: 15px;
  color: white;
  font-size: 20px;
}
/* Closable button inside the expanded image */
.closebtn {
  position: absolute;
  top: 10px;
  right: 15px;
  color: white;
  font-size: 35px;
  cursor: pointer;
}
  &lt;/style&gt;

